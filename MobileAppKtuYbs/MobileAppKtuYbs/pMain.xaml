<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileAppKtuYbs.pMain"
             xmlns:pages="clr-namespace:MobileAppKtuYbs">
    
  <FlyoutPage.Flyout>
    <pages:pMainFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
    
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:pMainDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
    
</FlyoutPage>